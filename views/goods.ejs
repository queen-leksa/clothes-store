<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <title>Clothes-store</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
<header>
    <div class="logo"><a href="/">Главная</a></div>
    <nav>
        <a href="">Категории</a>
        <a href="/add">Добавить товар</a>
        <a href="/goods">Все товары</a>
    </nav>
</header>
<main>
    <h1>Все товары</h1>
    <table>
        <tr>
            <% for (let name of names) { %>
                <th><%= name %></th>
            <% } %>
            <th></th>
        </tr>
        <% for (let i = 0; i < goods.length; i++) { %>
            <tr>
                <% for (let name of formNames) { %>
                    <td><%= goods[i][name] %></td>
                <% } %>
                <td><button data-type=<%= goods[i].article %>>Удалить</button></td>
            </tr>
        <% } %>
    </table>
</main>
</body>
</html>